ateRoute';\nimport PublicRoute from './Utils/PublicRoute';\nimport EmailAuth from './pages/EmailAuth';\nimport React, {Suspense} from \"react\";\nimport ReactLoading from 'react-loading';\n\nconst Example = ({type, color}) => (\n    <ReactLoading type={type} color={color} height={667} width={375}/>\n);\n\nfunction App() {\n    return (\n        <div className='App'>\n            <BrowserRouter>\n                <div className='content'>\n                    <Switch>\n                        <PublicRoute exact path='/' component={Home}/>\n                        <PublicRoute path='/signup' component={Signup2}/>\n                        <PrivateRoute path='/dashboard' component={Dashboard}/>\n                        <PublicRoute path='/validateToken' component={CompanyInformations}/>\n                        <PublicRoute path='/validateAuthToken' component={EmailAuth}/>\n                    </Switch>\n                </div>\n            </BrowserRouter>\n            <Toaster/>\n        </div>\n    );\n}\n\nexport default App;\n","import { configureStore } from '@reduxjs/toolkit';\nimport dashboardReducer from './dashboard';\n\nconst store = configureStore({\n    reducer: { dashboard: dashboardReducer }\n});\n\nexport default store;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport axios from 'axios';\nimport App from './App';\nimport './App.css';\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport './assets/styles/style.css';\nimport { Provider } from 'react-redux';\nimport store from './store/index';\n\n//axios.defaults.baseURL = 'http://demo.mypeerformance.com:4000/';\naxios.defaults.baseURL = 'http://93.188.167.68:4000';\n// axios.defaults.baseURL = 'http://192.168.1.108:4000';\n\nReactDOM.render(\n    <Provider store={store} >\n        <App />\n    </Provider>,\n    document.getElementById('root')); \n","// extracted by mini-css-extract-plugin"]}}